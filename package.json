{
  "name": "cs-club-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@monaco-editor/react": "^4.3.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.26.0",
    "bootstrap": "5",
    "firebase": "^9.6.6",
    "monaco-editor": "^0.31.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-monaco-editor": "^0.47.0",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "npm-run-all -p start:emulators start:site",
    "start:emulators": "NODE_ENV=development && firebase emulators:start --import=functions/.data --export-on-exit",
    "start:site": "NODE_ENV=development && react-scripts start",
    "deploy": "cd ./functions && npm run deploy",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
